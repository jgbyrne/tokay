Factor = @{
    Float
    '-' Expr            #-$Expr
    '(' Expr ')'
}

Term = @{
    Term '*' Factor     #$Expr * $Factor
    Term '/' Factor     #$Expr / $Factor
    Factor
}

Expr = @{
    Expr '+' Term       #$Expr + $Factor
    Expr '-' Term       #$Expr - $Factor
    Term
}

Expr                    #print $Expr
