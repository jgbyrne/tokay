Factor = @{
    Float
    '-' Expr            -$Expr
    '(' Expr ')'
}

Term = @{
    Term '*' Factor     $Expr * $Factor
    Term '/' Factor     $Expr / $Factor
    Factor
}

Expr = @{
    Expr '+' Term       $Expr + $Factor
    Expr '-' Term       $Expr - $Factor
    Term
}

Expr("ASK-13"+, "LS-1c", 13) \
                        print("Yes!")
